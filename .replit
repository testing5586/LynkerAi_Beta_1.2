entrypoint = "main.py"
modules = ["python-3.11", "postgresql-16", "nodejs-20"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "glibcLocales", "imagemagick_light", "libGL", "libiconv", "libxcrypt", "libyaml", "mailutils", "mesa", "openssl", "pkg-config", "rustc", "tesseract"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Admin Dashboard"

[[workflows.workflow]]
name = "Admin Dashboard"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd admin_dashboard && python app.py"
waitForPort = 5000

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 9000
externalPort = 9000

[[ports]]
localPort = 34241
externalPort = 3001

[[ports]]
localPort = 40015
externalPort = 3002

[[ports]]
localPort = 42489
externalPort = 3000
